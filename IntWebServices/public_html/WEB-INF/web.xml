<?xml version = '1.0' encoding = 'windows-1252'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
  <session-config>
    <session-timeout>120</session-timeout>
  </session-config>
  <servlet>
    <servlet-name>IntegrationBsLogComercialPort</servlet-name>
    <servlet-class>com.televisa.comer.integration.service.implementation.IntegrationBsLogComercial</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>IntegrationBsVentaModulosPort</servlet-name>
    <servlet-class>com.televisa.comer.integration.service.implementation.IntegrationBsVentaModulos</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>WsAuthImplPort</servlet-name>
    <servlet-class>com.televisa.comer.integration.service.implementation.WsAuthImpl</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>IntegrationBsPreemtionPort</servlet-name>
    <servlet-class>com.televisa.comer.integration.service.implementation.IntegrationBsPreemtion</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>IntegrationBsLogComercialPort</servlet-name>
    <url-pattern>/IntegrationBsLogComercialPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IntegrationBsVentaModulosPort</servlet-name>
    <url-pattern>/IntegrationBsVentaModulosPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WsAuthImplPort</servlet-name>
    <url-pattern>/WsAuthImplPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IntegrationBsPreemtionPort</servlet-name>
    <url-pattern>/IntegrationBsPreemtionPort</url-pattern>
  </servlet-mapping>
  <resource-ref>
      <description>Integration Paradigm Data Source</description>
      <res-ref-name>jdbc/MOR_DS_PG1</res-ref-name>
      <res-type>javax.sql.DataSource</res-type>
      <res-auth>Container</res-auth>
   </resource-ref>
</web-app>
